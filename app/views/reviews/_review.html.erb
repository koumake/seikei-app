<div class="more">
  <ul class="review-list">
    <li>
      <%= review.title%>
    </li>
    <li>
       <% review.images.each do |image| %>
         <%= image_tag image, class: 'images' %>
       <% end %>
    </li>
    <li>
      <%= review.treatment_site%>
    </li>
    <li>
      <%= review.date%>
    </li>
    <li>
      <%= review.clinic%>
    </li>
    
  </ul>
   <span>投稿者</span><%= review.user.nickname %>
   <%= link_to "詳細を見る", review_path(review)%>
</div>